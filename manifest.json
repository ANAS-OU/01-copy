{
  "manifest_version": 3,
  "name": "Copy Code Button",
  "version": "1.0",
  "description": "Adds a copy button to all <code> blocks.",
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://learn.zone01oujda.ma/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "assets/css/style.css"
      ]
    }
  ],
  "icons": {
    "48": "assets/img/icon.png"
  }
}